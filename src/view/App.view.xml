<mvc:View displayBlock="true"
    controllerName="ui5.typescript.helloworld.controller.FlexibleColumnLayout"
    height="100%"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:mvc="sap.ui.core.mvc">
    <App id="fcl">
        <pages>
            <!-- <Page><content><Text text="hello there" /></content></Page> -->
            <f:FlexibleColumnLayout id="flexibleColumnLayout"
                layout="TwoColumnsMidExpanded"
                backgroundDesign="Solid"
                busy="{busy>/app}"
                busyIndicatorDelay="0">
            </f:FlexibleColumnLayout>
        </pages>
    </App>
</mvc:View>